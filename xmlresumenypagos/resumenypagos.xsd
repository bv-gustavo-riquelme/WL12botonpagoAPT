														<xs:element name="EnUF" type="montoUF"/>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="Convenio">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Codigo" type="xs:positiveInteger"/>
											<xs:element name="Descripcion" type="descripcion"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="PagoRechazado">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="CodError" type="xs:positiveInteger"/>
								<xs:element name="Mensaje" type="descripcion"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Respuesta" default="OK">
		<xs:annotation>
			<xs:documentation>respuesta de un servicio</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="OK"/>
				<xs:enumeration value="NOK"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="XML" type="xs:string">
		<xs:annotation>
			<xs:documentation>mensaje que se envia a los medios de pagos</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ComprobantePagoCaja">
		<xs:annotation>
			<xs:documentation>informe de pago usado por caja</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Folio" type="xs:positiveInteger"/>
				<xs:element name="RutCliente" type="cod_persona"/>
				<xs:element name="NombreCliente" type="descripcion"/>
				<xs:element name="FechaPago" type="xs:date"/>
				<xs:element name="ValorUF">
					<xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:fractionDigits value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CanalPago">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="30"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="SucursalPago">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="60"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="InstrumentoPago">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="60"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ProductosPagados">
					<xs:complexType>
						<xs:sequence>
							<xs:sequence maxOccurs="unbounded">
								<xs:element name="Entrada">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Producto" type="producto"/>
											<xs:element name="NumCuota" type="xs:positiveInteger" minOccurs="0"/>
											<xs:element name="Cuota">
												<xs:complexType>
													<xs:sequence>
														<xs:element name="FechaVencimiento" type="xs:date"/>
														<xs:element name="Monto" type="montoCuota"/>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
										<xs:attribute name="contador" type="xs:positiveInteger" use="required"/>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:element name="TotalPagado">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="EnPesos" type="xs:nonNegativeInteger"/>
										<xs:element name="EnUF" type="montoUF"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
